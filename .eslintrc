{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [1, { "vars": "all", "args": "none" }],
    "quotes": [1, "single", "avoid-escape"]
  }
}
