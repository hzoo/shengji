{
  "name": "shengji",
  "version": "0.0.0",
  "description": "升级: a chinese trick-taking card game",
  "scripts": {
    "start": "node server",
    "dev": "npm start | $(npm bin)/babel-node hotload",
    "hotload": "$(npm bin)/webpack-dev-server --content-base build/ --progress --colors --hot --port 8090",
    "test": "$(npm bin)/babel-node $(npm bin)/tape test/*.js",
    "postinstall": "$(npm bin)/webpack",
    "lint": "jscs . --verbose && jshint . --exclude-path .gitignore",
    "coverage": "$(npm bin)/istanbul cover arg.js test/*.js --report lcovonly",
    "coverage-html": "$(npm bin)/istanbul cover arg.js test/*.js --report html",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | $(npm bin)/coveralls && rm -rf ./coverage",
    "docs": "$(npm bin)/jsdoc -c conf.json",
    "ci": "npm run lint && npm test && npm run coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hzoo/shengji.git"
  },
  "keywords": [
    "shengji",
    "tuolaji",
    "bashifen",
    "tractor",
    "zhaopengyou",
    "chinese",
    "cardgame"
  ],
  "author": {
    "name": "Henry Zhu",
    "url": "http://henryzoo.com"
  },
  "engines": {
    "iojs": "1.0.x"
  },
  "license": "MIT",
  "dependencies": {
    "babel": "^4.5.1",
    "babel-loader": "^4.0.0",
    "autoprefixer-core": "^5.1.5",
    "css-loader": "^0.9.0",
    "express": "~4.11.0",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.12.2",
    "lodash": "^3.1.0",
    "postcss-loader": "^0.3.0",
    "react": "~0.12.2",
    "reflux": "~0.2.1",
    "socket.io": "~1.3.2",
    "socket.io-client": "~1.3.2",
    "style-loader": "^0.8.2",
    "url-loader": "^0.5.5",
    "webpack": "~1.6.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "esprima-fb": "^13001.1.0-dev-harmony-fb",
    "file-loader": "^0.8.1",
    "istanbul": "^0.3.5",
    "jscs": "^1.11.3",
    "jsdoc": "^3.3.0-alpha13",
    "jshint": "^2.6.0",
    "nodemon": "^1.3.6",
    "react-hot-loader": "^1.1.5",
    "tape": "^3.5.0",
    "webpack-dev-server": "^1.7.0"
  },
  "bugs": {
    "url": "https://github.com/hzoo/shengji/issues"
  },
  "homepage": "https://github.com/hzoo/shengji"
}
